import time
start = time.perf_counter()

from utility_scripts.scraper_utils import pull_gamelogs, get_player_hrefs

import string
letters = list(string.ascii_uppercase)


href_list = get_player_hrefs(letters)
qb, rb, wr, te  = pull_gamelogs(href_list)

qb.to_csv('dev_files/test_game_log_QB.csv', index=False)
rb.to_csv('dev_files/test_game_log_RB.csv', index=False)
wr.to_csv('dev_files/test_game_log_WR.csv', index=False)
te.to_csv('dev_files/test_game_log_TE.csv', index=False)