# import time
# start = time.perf_counter()

from utility_scripts.scraper_utils import pull_gamelogs, get_player_hrefs

import string
letters = list(string.ascii_uppercase)


href_pdf = get_player_hrefs(letters)
print(f"Total Player Count to Pull: {href_pdf.shape[0]}")
print(href_pdf)
results = pull_gamelogs(href_pdf)

results["QB_gamelogs"].to_csv('scraped_data/SB_ERA_game_logs_QB.csv', index=False)
results["RB_gamelogs"].to_csv('scraped_data/SB_ERA_game_logs_RB.csv', index=False)
results["WR_gamelogs"].to_csv('scraped_data/SB_ERA_game_logs_WR.csv', index=False)
results["TE_gamelogs"].to_csv('scraped_data/SB_ERA_game_logs_TE.csv', index=False)

